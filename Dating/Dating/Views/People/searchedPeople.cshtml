@model IEnumerable<Dating.Models.Person>


    <div class="topnav">
        <input type="text" placeholder="Search People">
        <input type="button"
               value="Submit"
               onclick="location.href='@Url.Action("SearchedPeople", "People")'" />
    </div>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FirstName)
        </th>
   
        <th></th>
    </tr>

@foreach (var item in Model) {

<tr>
    <th>
        @Html.DisplayFor(modelItem => item.FirstName)
    </th>

    <td>
        @Html.ActionLink("Back to ", "Details") |
    </td>
</tr>
}

</table>
